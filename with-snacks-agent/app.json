{
  "expo": {
    "name": "Snack Agent",
    "slug": "with-snacks-agent",
    "version": "1.0.0",
    "scheme": "snack-agent",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
